<script>
    let output =[];
    let buffer = 1;

 function fizzbuzz(){
    if (buffer%5==0 && buffer%3==0){
        output.push("fizzbuzz");}
    else if (buffer%5==0){
        output.push("buzz");
    }
    else if (buffer%3==0){
        output.push("fizz");
    }
    else{
        output.push(buffer);
    }
    console.log(output);
    buffer++;
    }

function popbuzz(){
   for(let i=0; i<output.length;i++){
      if (output[i]%7==0){
        output.pop(output[i]);
      }
   }
   console.log('bbbb');
    console.log(output);
}
</script>
